<!DOCTYPE html>
   <html xmlns:ng="http://angularjs.org" >
     <head>
       <title>XX :: <%= title %> :: </title>
     </head>
     <body>
      	<% if ( user ) { %>
           <h2>Hello <%=user%> </h2>
           <a href="/signout">Sign out</a>
         <% } else { %>
           <a href="/signup">Signup</a>
           <a href="/signin">Signin</a>
		<% } %>
     	<img src="/public/img/nodeJS.png" alt="Logo"> <br />
       <h1><%= title %></h1>
       
     	
     	<section ng-view></section>
     	<script type="text/javascript">
     		window.user = <%- user || 'null' %>;
		</script>
   
     	<script type="text/javascript" src="/public/lib/angular/angular.js"></script>
     	<script type="text/javascript" src="/public/lib/angular-route/angular-route.js"></script>
     	
     	<script type="text/javascript" src="/example/example.client.module.js"></script>
     	<script type="text/javascript" src="/example/controllers/example.client.controller.js"></script>
     	
     	<script type="text/javascript" src="/public/users/users.client.module.js"></script>
     	<script type="text/javascript" src="/public/users/services/authentication.client.service.js"></script>
     	
     	
     	<script type="text/javascript" src="/public/application.js"></script>
     </body>
</html>